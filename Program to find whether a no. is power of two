#include <stdio.h>
int isPowerOfTwo(int n) {
    if (n == 0) {
        return 0;
    }
    return !(n & (n - 1)); 
}
int main() {
    int num = 16;
    if (isPowerOfTwo(num)) {
        printf("%d is a power of two\n", num);
    } else {
        printf("%d is not a power of two\n", num);
    }
    return 0;
}
